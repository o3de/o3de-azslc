        float m_f9;  //<-- Dummy float2 must be inserted before this variable.
        float3x3 m_mat10;
        float m_f11;  //<-- Dummy float2 must be inserted before this variable.
        float4x3 m_mat12;
        float m_f13;  //<-- Dummy float2 must be inserted before this variable.
    } m_d14;

    float m_f15;
    float2x2 m_mat16;
};

struct B
{
    float m_f17;
    float3x2 m_mat18;
};

struct C
{
    float m_f19;
