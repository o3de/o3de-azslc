// For glow module.

float GlowFwd(float ycIn, float glowGainIn, float glowMid)
{
    float glowGainOut;

    if (ycIn <= 2.0 / 3.0 * glowMid)
    {
        glowGainOut = glowGainIn;
    }
    else if (ycIn >= 2.0 * glowMid)
    {
        glowGainOut = 0.0;
    }
    else
    {
        glowGainOut = glowGainIn * (glowMid / ycIn - 1.0 / 2.0);
    }

    return glowGainOut;
}

