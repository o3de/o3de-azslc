    float hue = RGBToHue(aces);
    float centeredHue = CenterHue(hue, RRT_RED_HUE);
    float hueWeight = CubicBasisShaper(centeredHue, RRT_RED_WIDTH);

    aces[0] = aces[0] + hueWeight * saturation *(RRT_RED_PIVOT - aces[0]) * (1.0 - RRT_RED_SCALE);

