float MaxF3(float3 In)
{
    return max(In.r, max(In.g, In.b));
}

float MinF3(float3 In)
{
    return min(In.r, min(In.g, In.b));
}

float RGBToSaturation(float3 rgb)
{
    return (max(MaxF3(rgb),  10e-5f ) - max(MinF3(rgb),  10e-5f )) / max(MaxF3(rgb), 1e-2);
