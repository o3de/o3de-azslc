// It is an error to use [[pad_to(N)]] inside a class.

ShaderResourceGroupSemantic slot1
{
    FrequencyId = 1;
};

class MyClass
{
    float m_red;
    [[pad_to(16)]]
};

ShaderResourceGroup DemoSrg : slot1
{
    MyClass m_data;
};

float4 PSMain() : SV_Target0
{
    float4 color = float4(0, 0, 0, 0);
    color.r = DemoSrg::m_data.m_red;
    return color;
}